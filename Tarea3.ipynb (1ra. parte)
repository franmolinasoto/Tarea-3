    {
      "cell_type": "code",
      "source": [
        "def v_R_starsNuevo(R, M_1, R1):\n",
        "  v_R_starsNuevo = ((np.sqrt((G*M_1)/R)))/1000\n",
        "  return v_R_starsNuevo\n",
        "\n",
        "v_r_starsNuevo = v_R_starsNuevo(R, M_1, R_1)\n",
        "\n",
        "# Haga el gráfico :)\n",
        "plt.plot(R,v_r_starsNuevo)\n",
        "plt.xlabel('R [m]')\n",
        "plt.ylabel('v [km/s]')\n",
        "plt.legend()\n",
        "plt.show()\n",
        "\n",
        "\n",
        "\n"
